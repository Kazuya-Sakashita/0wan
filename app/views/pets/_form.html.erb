<%= form_with model: @pet, local: true do |form| %>
<p><%= form.label :name %><br>
<%= form.text_field :name %> 
</p>

<p><%= form.label :age %><br>
<%= form.text_field :age %> 
</p>

<p><%= form.label :description %><br>
<%= form.text_area :description %>
</p>

<p><%= form.label :image1 %><br>
<%= form.text_field :image1 %> 
</p>



<p><%= form.submit '投稿' %>
</p>

<% end %>